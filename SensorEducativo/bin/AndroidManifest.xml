<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.edu.uvg.besser"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.edu.uvg.besser.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.edu.uvg.besser.DesplegarUsuario"
            android:label="@string/title_activity_desplegar_usuario"
            android:parentActivityName="org.edu.uvg.besser.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.edu.uvg.besser.MainActivity" />
        </activity>
        <activity
            android:name="org.edu.uvg.besser.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="org.edu.uvg.besser.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.edu.uvg.besser.DesplegarUsuario" />
        </activity>
        <activity
            android:name="org.edu.uvg.besser.MostrarTema"
            android:label="@string/title_activity_mostrar_tema"
            android:parentActivityName="org.edu.uvg.besser.DisplayMessageActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.edu.uvg.besser.DisplayMessageActivity" />
        </activity>
        <activity
            android:name="org.edu.uvg.besser.MostrarEjercicios"
            android:label="@string/title_activity_mostrar_ejercicios"
            android:parentActivityName="org.edu.uvg.besser.MostrarTema" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.edu.uvg.besser.MostrarTema" />
        </activity>
        <activity
            android:name="org.edu.uvg.besser.ActivityResultado"
            android:label="@string/title_activity_activity_resultado"
            android:parentActivityName="org.edu.uvg.besser.MostrarTema" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.edu.uvg.besser.MostrarTema" />
        </activity>
        <activity
            android:name="org.edu.uvg.besser.MostrarProfesor"
            android:label="@string/title_activity_mostrar_profesor"
            android:parentActivityName="org.edu.uvg.besser.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="org.edu.uvg.besser.MainActivity" />
        </activity>
        <activity
            android:name="org.edu.uvg.besser.EditarEjerciciosSeleccionandoTema"
            android:label="@string/title_activity_editar_ejercicios_seleccionando_tema" >
        </activity>
        <activity
            android:name="org.edu.uvg.besser.EditarSeleccionEjercitacion"
            android:label="@string/title_activity_editar_seleccion_ejercitacion" >
        </activity>
        <activity
            android:name="org.edu.uvg.besser.EditarEjercicios"
            android:label="@string/title_activity_editar_ejercicios" >
        </activity>
        <activity
            android:name="org.edu.uvg.besser.EditarEjercitacionesTema"
            android:label="@string/title_activity_editar_ejercitaciones_tema" >
        </activity>
        <activity
            android:name="org.edu.uvg.besser.EditarEjercitacion"
            android:label="@string/title_activity_editar_ejercitacion" >
        </activity>
        <activity
            android:name="org.edu.uvg.besser.EditarTema"
            android:label="@string/title_activity_editar_tema" >
        </activity>
    </application>

</manifest>
